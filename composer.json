{
  "name": "freemh/dailycomfort",
  "type": "wordpress-theme",
  "license": "MIT",
  "authors": [
    {
      "name": "Arbaoui Mehdi",
      "email": "arbaoui.mehdi@gmail.com"
    }
  ],
  "description": "Daily Comfort Woocommerce Theme",
  "homepage": "https://arbaouimehdi.com/daily-comfort",
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    },
    {
      "type": "composer",
      "url": "https://wp-languages.github.io"
    }
  ],
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "require": {
    "php": ">=7",
    "composer/installers": "~1.0",
    "illuminate/support": "~5.4",
    "roots/sage-lib": "~9.0.0-beta.4",
    "soberwp/controller": "~9.0.0-beta.4",

    "humanmade/s3-uploads": "*",
    "wpackagist-plugin/woocommerce": "3.2.6",
    "wpackagist-plugin/woocommerce-services": "1.9.1",
    "wpackagist-plugin/contact-form-7": "4.9.2",
    "wpackagist-plugin/redis-cache": "*",
    "wpackagist-plugin/imagify": "*",
    "wpackagist-plugin/wordfence": "*"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^2.8.0"
  },
  "extra": {
    "installer-paths": {
      "../../plugins/{$name}/": ["type:wordpress-plugin"]
    }
  }
}
